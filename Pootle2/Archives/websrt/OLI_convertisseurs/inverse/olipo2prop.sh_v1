#!/bin/bash
# oliprop2po.sh

for file in `ls ../*.po`
do
 newname=`basename $file .po`
 templatename=${file%_*po}
 templatename=${templatename%_*}"_en.properties"
 echo 'File to be processed: ' $newname
 echo 'Template name: ' $templatename
 po2prop -i $file -o $newname.properties -t $templatename
done
